  <select id="getMenuByUserId" resultType="crm.com.model.SysMenu">
		SELECT m.id,m.name,m.parentid,m.iconCls,m.url,
			(select count(1) from sysmenu s where s.parentId!=0 and s.parentId=m.id) countChildrens 
		FROM sysmenu m
		  LEFT JOIN sysrolemenu rm
		  ON m.id = rm.menu_id
		  LEFT JOIN sysrole r
		  ON rm.role_id = r.id
		  LEFT JOIN sysuserrole ur
		  ON  r.id = ur.roleId
		  LEFT JOIN sysuser u
		  ON ur.userId = u.id
		  WHERE m.enable=1 AND u.id = 1
		  AND m.id NOT IN(
			SELECT tt.id
			FROM
				(SELECT m.id,
					(SELECT COUNT(parentId) FROM sysmenu sp WHERE sp.enable=1 AND sp.parentId=m.id AND m.parentId=0 ) countch
				 FROM sysmenu m WHERE m.parentId=0
		  		) tt 
		  	where countch =0
		  )
		  ORDER BY m.sequence
	</select> 
